generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./db.sqlite"
}

model dias {
  id      Int       @id @default(autoincrement())
  dia     String    @unique
}

model eventos {
  id             Int    @id @default(autoincrement())
  desc           String
  veiculo        String
  responsavel    String
  dataEvento     String
  dataRegistrado String
  diaId          Int
  diaOrdem       Int
  titulo         String
  funcionarios   String
  propColor      String    
}

model carros {
  id   Int    @id @default(autoincrement())
  nome String
}

model funcionarios {
  id   Int    @id @default(autoincrement())
  nome String
}

model responsaveis {
  id   Int    @id @default(autoincrement())
  nome String
}
